WHAT: none
WHERE: none
WHEN: 0-24
WHO: none
OTHER: none

    If isWithCompanion()
        endDate()
    Endif
    skag_stage.setGlobal(0)
    SceneStart()
    "Comment trouver la façon d'éveiller son côté lesbienne?"
    selectNPC()
    Actor = getSelectedNPC()
    
    If !Actor.isValid() || Actor.isCreature() || Actor.isMale()
        "Aucun NPC sélectionné. La quête n'a donc pus commencé."
        QuestEnd()
    Else
        Actor.dress()
        Actor.show(2)
        "Je suis certain que <Actor.name> est dans le dénie et je veux l'aider à faire son coming-out."
        "Qui devrais-je présenter à <Actor.name>, pour l'aider à éveiller son côté lesbienne?"
        0:: !Player.isMale():: "Moi, c'est clair!"
        1:: "Je pense savoir qui"
        
        If 0
            Actor2 = Player
        Else
            selectNPC()
            Actor2 = getSelectedNPC()       
        Endif
        
        If !Actor2.isValid() || Actor2.isCreature() || Actor2.isMale()
            "Personne non valide pour corrompre <Actor.name>. La quête ne peu donc pas débuter."
            QuestEnd()
        Else
            AggA.setActorAlias(Actor2)
            VicA.setActorAlias(Actor)
            
            B1.generateBuildingAlias(sports_centre)
            B1.addObjective(skag_awaken)
            skag_stage.setGlobal(1)
        Endif        
    Endif
    SceneEnd()